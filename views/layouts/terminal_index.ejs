                                    <select class="form-control filter_meter selectpicker" data-live-search="true"
                                        required id="filter_tram" autocomplete="off" data-dropup-auto="false">
                                        <option value="all" dv_type="all" <%=(parent_id==='all' ) ? 'selected' : '' %>
                                            >Các đơn vị quản lý</option>
                                        <% 
                                        const groups=danhsachtram.filter(item=>
                                            item.donviquanly_id === null);
                                            groups.forEach(group => {
                                            const subItems = danhsachtram.filter(item => item.donviquanly_id ===
                                            group.id);
                                        %>
                                            <!-- Nhóm chính -->
                                            <option value="<%= group.id %>" dv_type="dvql" <%=(parent_id==='dvql' +
                                                group.id) ? 'selected' : '' %> style="font-weight: bold;"><%= group.name
                                                    %>
                                            </option>
                                            <!-- Các trạm con -->
                                            <% subItems.forEach(tram=> { %>
                                                <option value="<%= tram.id %>" dv_type="dvnd" <%=(parent_id==='dvnd' +
                                                    tram.id) ? 'selected' : '' %>>&nbsp;&nbsp;&nbsp;<%= tram.name %>
                                                </option>
                                                <% }); %>
                                                    <% }); %>
                                    </select>
